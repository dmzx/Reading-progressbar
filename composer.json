{
	"name": "dmzx/progressbar",
	"type": "phpbb-extension",
	"description": "This extension will add a reading progressbar, selectable position.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.1",
	"time": "2020-01-16",
	"keywords": [
		"phpbb",
		"extension",
		"progressbar"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		},
		{
			"name": "martin",
			"homepage": "https://www.martins-phpbb.com",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"composer/installers": "~1.0.0"
	},
	"extra": {
		"display-name": "Reading progressbar",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<4.4.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "progressbar_version.json",
			"ssl": true
		}
	}
}